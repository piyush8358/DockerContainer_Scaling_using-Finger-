   "id": "fc4881db",
   "metadata": {},
   "outputs": [],
   "source": [
    "cap=cv2.VideoCapture(0)\n",
    "while True:\n",
    "    ret, photo=cap.read()\n",
    "    cv2.imshow(\"my photo\",photo)\n",
    "    \n",
    "    if cv2.waitKey(10)==ord(\"a\"):\n",
    "        break\n",
    "# press a for closing video window\n",
    "\n",
    "cv2.destroyAllWindows()"
   ]
  },
